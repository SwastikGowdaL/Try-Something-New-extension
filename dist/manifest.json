{
    "manifest_version": 2,
    "name": "try-something-new",
    "version": "1.0.0",
    "description": "Suggests a new activity to do each and every time.",
    "browser_action": {
      "default_popup": "index.html"
    },
    "icons":{
      "16": "Icon-16.png",
      "64": "Icon-64.png",
     "128": "Icon-128.png"
    },

    "content_security_policy": "script-src 'self' https://unpkg.com ; object-src 'self'"
  }
